<%= form_for(@search) do |f| %>

  <div class="field">
    <%= f.label :address %><br />
    <%= f.text_field :address, :value => "e.g. 1 Times Square", :id => "default_address", :class => "example_text" %>
  </div>
  <div class="field">
    <%= f.label :city %><br />
    <%= f.text_field :city, :value => "New York" %>
  </div>
  <div class="field">
    <%= f.label :state %><br />
    <%= f.text_field :state, :value => "New York" %>
  </div>
  <div class="field">
    <%= f.label :country %><br />
    <%= f.text_field :country, :value => "USA" %>
  </div>
  <div class="field">
    <%= f.hidden_field :latitude %>
    <%= f.hidden_field :longitude %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
